{
  "name": "Toggle Tor",
  "short_name": "SVEX",
  "version": "0.1",
  "description": "Toggle on an off proxy configuration for tor",
  "author": "Max Schulte",
  "permissions": [
	  "proxy"
  ],
  "browser_action": {
    "default_popup": "dist/index.html",
    "default_icon": {
      "16": "dist/icon-16.png",
      "32": "dist/icon-32.png",
      "48": "dist/icon-48.png",
      "128": "dist/icon-128.png"
    }
  },
  "manifest_version": 2
}
